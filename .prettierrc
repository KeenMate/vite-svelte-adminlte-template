{
  "useTabs": true,
  "singleQuote": false,
  "semi": false,
  "bracketSpacing": false,
  "trailingComma": "none",
  "printWidth": 100,
  "plugins": [
    "prettier-plugin-svelte"
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "semi": false,
        "singleQuote": false
      }
    }
  ]
}
